%class.A = type { i8 (...)**, i32* }

define i32 A__field_const_index() {
entry:
    %1 = bitcast i8* %0 to i32**
    %2 = getelementptr inbounds i32, i32* %1, i32 2
    store i32 10, i32* %2
    ret i32 1
}

define i32 A__field_var_index(i32 %1) {
entry:
    %2 = bitcast i8* %0 to i32**
    %3 = add i32 %1, 1
    %4 = getelementptr inbounds i32, i32* %2, i32 %3
    store i32 10, i32* %4
    ret i32 2
}

define i32 A__local_const_index() {
    %1 = alloca i32*, align 8
entry:
    %2 = call noalias i8* @calloc(i32 1, i32 84)
    %3 = bitcast i8* %2 to i32*
    store i32* %3, i32** %1
    %4 = load i32*, i32** %1, align 8
    %5 = getelementptr inbounds i32, i32* %4, i32 4
    store i32 7, i32* %5
    ret i32 3
}

define i32 A__local_var_index(i32 %1) {
    %2 = alloca i32*, align 8
entry:
    %3 = call noalias i8* @calloc(i32 1, i32 84)
    %4 = bitcast i8* %3 to i32*
    store i32* %4, i32** %2
    %5 = load i32*, i32** %2, align 8
    %6 = add i32 %1, 1
    %7 = getelementptr inbounds i32, i32* %5, i32 %6
    store i32 7, i32* %7
    ret i32 4
}

